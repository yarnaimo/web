{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "repository": "github:yarnaimo/web",
  "license": "MIT",
  "author": "yamaimo <yarnaimo@gmail.com>",
  "main": "dist/server/index.js",
  "scripts": {
    "build": "tsc --project tsconfig.server.json",
    "debug": "node -r ts-node/register --inspect-brk",
    "deploy": "ts-node server/scripts/migrate.ts && firebase deploy",
    "logs": "firebase functions:log",
    "next:build": "NODE_ENV=production next build",
    "next:debug": "node -r ts-node/register --inspect server/next-dev",
    "next:dev": "ts-node --project tsconfig.server.json server/next-dev",
    "next:export": "NODE_ENV=production next export",
    "np": "np --no-cleanup --no-publish",
    "prepare": "cp node_modules/modern-normalize/modern-normalize.css src/styles/_modern-normalize.scss && cp node_modules/material-colors/dist/colors.var.css src/styles/_material-colors.scss",
    "serve": "serve -n -p 5005 & firebase emulators:start --only firestore",
    "serve-f": "yarn build && firebase emulators:start --only firestore,functions",
    "shell": "yarn build && firebase functions:shell",
    "storybook": "start-storybook -p 6006 --ci",
    "test": "NODE_ENV=test jest",
    "ts-node": "ts-node"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@juggle/resize-observer": "^3.1.3",
    "@next/bundle-analyzer": "^9.3.4",
    "@yarnaimo/rain": "^0.5.2",
    "@yarnaimo/twimo": "^3.0.2",
    "babel-plugin-emotion": "^10.0.33",
    "color-convert": "^2.0.1",
    "express": "^4.17.1",
    "feather-icons": "^4.28.0",
    "material-colors": "^1.2.6",
    "modern-normalize": "^0.6.0",
    "next": "^9.3.4",
    "next-fonts": "^1.0.3",
    "next-pwa": "^2.3.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^2.7.0",
    "react-transition-group": "^4.3.0",
    "react-use": "^14.0.0",
    "rmwc": "^5.7.2",
    "sass": "^1.26.3",
    "ulid": "^2.3.0",
    "use-media": "^1.4.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-typescript": "^7.9.4",
    "@firebase/testing": "^0.18.2",
    "@storybook/react": "^5.3.18",
    "@types/color-convert": "^1.9.0",
    "@types/express": "^4.17.4",
    "@types/feather-icons": "^4.7.0",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/react-transition-group": "^4.2.4",
    "@yarnaimo/tss": "^0.2.0",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "jest-environment-uint8array": "^1.0.0",
    "sass-loader": "^8.0.2",
    "serve": "^11.3.0"
  },
  "engines": {
    "node": "10"
  }
}